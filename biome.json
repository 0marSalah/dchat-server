{
  "$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "files": {
    "ignore": [".github/", "dist/"]
  },
  "javascript": {
    "formatter": {
      "enabled": true,
      "indentStyle": "space",
      "quoteStyle": "single"
    }
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space"
  },
  "linter": {
    "enabled": true,
    "rules": {
      "a11y": {
        "noRedundantRoles": "off",
        "useSemanticElements": "off"
      },
      "recommended": true,
      "correctness": {
        "useJsxKeyInIterable": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      },
      "suspicious": {
        "noShadowRestrictedNames": "off",
        "noExplicitAny": "off"
      },
      "complexity": {
        "noStaticOnlyClass": "off"
      }
    }
  }
}
